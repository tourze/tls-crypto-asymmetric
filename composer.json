{
  "name": "tourze/tls-crypto-asymmetric",
  "description": "TLS非对称加密算法实现",
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-hash": "*",
    "ext-openssl": "*",
    "ext-sodium": "*",
    "paragonie/sodium_compat": "^2.0",
    "phpseclib/phpseclib": "^3.0.43",
    "tourze/tls-common": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/framework-bundle": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\TLSCryptoAsymmetric\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\TLSCryptoAsymmetric\\Tests\\": "tests/"
    }
  }
}